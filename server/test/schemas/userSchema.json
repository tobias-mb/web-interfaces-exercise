{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://example.com/object1613070276.json", 
	"title": "Root", 
	"type": "object",
	"required": [
		"id",
		"username",
		"email"
	],
	"properties": {
		"id": {
			"$id": "#root/id", 
			"title": "Id", 
			"type": "integer",
			"examples": [
				1
			],
			"default": 0
		},
		"username": {
			"$id": "#root/username", 
			"title": "Username", 
			"type": "string",
			"default": "",
			"examples": [
				"tester"
			],
			"pattern": "^.*$"
		},
		"email": {
			"$id": "#root/email", 
			"title": "Email", 
			"type": "string",
			"default": "",
			"examples": [
				"spammail-account@gmx.de"
			],
			"pattern": "^.*$"
		}
	}
}
